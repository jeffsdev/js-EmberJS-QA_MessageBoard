<div class="ask-section">
  {{new-question saveQuestion="saveQuestion"}}
  <!-- Obelisk background image: "Obelisk", by erenarik at DeviantArt-->
</div>
    <!-- {{#link-to 'favorited'}}Favorites{{/link-to}}     -->
<div class="question-feed">
  <h2>Recent Questions</h2>
  <ul>
    {{#each model.questions as |question|}}
    {{question-tile question=question upvote="upvote" downvote="downvote"}}

    {{/each}}
  </ul>
</div>
